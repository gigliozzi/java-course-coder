package classe;

public class DataTeste {

	public static void main(String[] args) {
		
		Data d1 = new Data();
		Data d2 = new Data();
		
		d1.dia = 5;
		d1.mes = 8;
		d1.ano = 2006;
		
		System.out.printf("%d/%d.2f/%d\n", d1.dia, d1.mes, d1.ano);

		d2.dia = 13;
		d2.mes = 6;
		d2.ano = 2013;
		
		System.out.println(d1.dia +" de "+ d1.mes+" de "+ d1.ano);
		

	}

}
