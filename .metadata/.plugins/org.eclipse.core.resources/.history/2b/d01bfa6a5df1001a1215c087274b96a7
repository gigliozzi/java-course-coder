package classe;

public class DataTeste {

	public static void main(String[] args) {
		
		Data d1 = new Data();
		Data d2 = new Data();
		
		d1.dia = 5;
		d1.mes = 08;
		d1.ano = 2006;
		
		System.out.printf("%d");

		d2.dia = 13;
		d2.mes = 06;
		d2.ano = 2013;
		
		System.out.println(d1.dia +" de "+ d1.mes+" de "+ d1.ano);
		

	}

}
